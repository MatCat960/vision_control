<?xml version="1.0"?>
<launch>
    <arg name="ROBOTS_NUM" default="3" />
    <arg name="NAME" default="hummingbird" />
    <arg name="posfile" default="$(find formation_control)/pos.txt"/>
    <arg name="GUI" default="true"/>

    <arg name="use_robot_0" default="1"/>
    <arg name="use_robot_1" default="1"/>
    <arg name="use_robot_2" default="1"/>
    <arg name="use_robot_3" default="1"/>
    <arg name="use_robot_4" default="1"/>
    <arg name="use_robot_5" default="1"/>
    <arg name="use_robot_6" default="1"/>
    <arg name="use_robot_7" default="1"/>
    <arg name="use_robot_8" default="1"/>
    <arg name="use_robot_9" default="1"/>
    <arg name="use_robot_10" default="1"/>
